<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>dao_service</servlet-name>
    <servlet-class>com.warehouse.server.DAOService</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>dao_service</servlet-name>
    <url-pattern>/warehouse/dao</url-pattern>
  </servlet-mapping>
  
  <!-- Defapresentpage to serve -->
  <welcome-file-list>
    <welcome-file>Warehouse.html</welcome-file>
  </welcome-file-list>

</web-app>

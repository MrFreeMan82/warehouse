<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

    <b:Form ui:field="form">
        <b:FieldSet>
            <b:FormGroup>
                <b:FormLabel ui:field="lblUserType" for="userTypeID"/>
                <b:ListBox ui:field="userTypeListBox" b:id="userTypeID"/>
            </b:FormGroup>

            <b:FormGroup>
                <b:FormLabel ui:field="lblUserName" for="userNameID"/>
                <b:InlineHelpBlock iconType="EXCLAMATION_TRIANGLE" />
                <b:TextBox ui:field="txtUserName" b:id="userNameID"/>
            </b:FormGroup>

            <b:FormGroup>
                <b:FormLabel ui:field="lblPassword" for="passwordID"/>
                <b:InlineHelpBlock iconType="EXCLAMATION_TRIANGLE" />
                <b:Input type="PASSWORD" ui:field="txtPassword" b:id="passwordID" />
            </b:FormGroup>
        </b:FieldSet>
    </b:Form>
</ui:UiBinder>
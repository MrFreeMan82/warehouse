<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

    <b:Form ui:field="form">
        <b:FieldSet>
            <b:FormGroup>
                <b:FormLabel ui:field="groupNameLabel" for="groupNameTextID"/>
                <b:InlineHelpBlock iconType="EXCLAMATION_TRIANGLE" />
                <b:TextBox ui:field="groupNameText" b:id="groupNameTextID"/>
            </b:FormGroup>
        </b:FieldSet>
    </b:Form>
</ui:UiBinder>
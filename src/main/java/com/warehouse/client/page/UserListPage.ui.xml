<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

    <g:VerticalPanel width="100%" ui:field="listPanel">
        <b:Navbar>
            <b:NavbarHeader>
                <b:NavbarBrand>User List</b:NavbarBrand>
                <b:NavbarCollapseButton dataTarget="#navbar-collapse"/>
            </b:NavbarHeader>

            <b:NavbarCollapse b:id="navbar-collapse">
                <b:NavbarNav>
                    <b:AnchorListItem ui:field="newUser" b:id="newUserAnchorListItem">New</b:AnchorListItem>
                    <b:AnchorListItem>Edit</b:AnchorListItem>
                    <b:AnchorListItem>Delete</b:AnchorListItem>
                </b:NavbarNav>
                <b:NavbarForm pull="LEFT">

                    <b:InputGroup>
                        <b:TextBox placeholder="Search" autoComplete="false"/>
                        <b:InputGroupButton>
                            <b:Button text="..."/>
                        </b:InputGroupButton>
                    </b:InputGroup>
                </b:NavbarForm>
            </b:NavbarCollapse>
        </b:Navbar>
        <!-- Table UserListPresentList.java -->
    </g:VerticalPanel>
</ui:UiBinder>
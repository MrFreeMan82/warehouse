<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
  "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to='warehouse'>
  <!-- Inherit our applications main module.                      -->
  <inherits name='com.warehouse.Warehouse'/>

  <source path="com.warehouse"/>

  <entry-point class="com.warehouse.WarehouseTest"/>

  <!-- Specify the path to any remote services.                   -->
  <servlet path="/warehouse/authorize" class="com.warehouse.server.services.LoginService" />

</module>
